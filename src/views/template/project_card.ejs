<% subjects.forEach(function(subject){ %>
    <h3><%= subject.name %></h3>
    <div class="row justify-content-center">
    <% subject.projects.forEach(function(project){ %>
        <div class="col-lg-3 col-md-6 col-sm-12 mb-2">
            <div class="card border border-primary">
                <img class="card-img-top" src="/public/img/codigo.jpg" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title"><%= project.project_name %></h5>
                    <p class="card-text"><strong>Responsable: </strong><%= project.responsable_member %></p>
                    <a href="#" class="btn btn-primary">Ver proyecto</a>
                </div>
            </div>
        </div>
        <% }); %>
    </div>
<% }); %>
      