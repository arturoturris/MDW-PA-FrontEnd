<div class="row">
<% for(let subject of subjects) { %>
    <div class="col-lg-3 col-md-6 col-sm-12 mb-2">
        <div class="card border border-primary">
            <img class="card-img-top" src="/public/img/codigo.jpg" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title"><%= subject.nombre %></h5>
                <p class="card-text"><strong>Periodo: </strong><%= subject.Pnombre %></p>
                <p class="card-text"><strong>NRC: </strong><%= subject.nrc %></p>
                <a href="/profesor/<%= subject.nrc %>" class="btn btn-primary">Ver proyectos activos</a>
            </div>
        </div>
    </div>
<% } %>
</div>
      